{
  "expo": {
    "name": "SGV School",
    "slug": "sgv-school",
    "version": "2.0.0",
    "icon": "./assets/images/icon.png",
    "scheme": "sgvschool",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sgvschool.app"
    },
    "android": {
      "package": "com.sgvschool.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 5,
      "enableEdgeToEdge": true,
      "enable16KbPageSize": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4d8aa0f7-ce1f-46c3-83ae-fa188aad9ebe"
      }
    }
  }
}
